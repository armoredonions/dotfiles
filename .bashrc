then
 	net_address=$(curl ifconfig.io --silent)
	echo -e "${IYellow}Welcome to the Net, Choom. Watchout for rogue AI..."
	echo -e "${IRed}üõ°Ô∏è  NetWatch has identified you as: ${URed}$net_address$IRed üõ°Ô∏è"
else
	echo "Looks like your on an isolated net...scanning for netrunners."
	echo "Failed to identify any netrunners on this subnet, ping protocol may need updating."
fi

